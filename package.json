{
    "name": "unlock-client",
    "version": "2.0.0",
    "description": "A frontend javascript library for authenticating with Unlock",
    "keywords": [
        "authentication",
        "frontend",
        "unlock",
        "websocket"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/unlock.js",
    "scripts": {
        "test": "npm run test:lib; npm run test:min",
        "test:lib": "mocha-headless-chrome -f ./test/index.html",
        "test:min": "mocha-headless-chrome -f ./test/min.html",
        "lint:src": "eslint src/*.js -c .eslint.json",
        "clean": "rm -rf  dist",
        "dev": "parcel watch src/unlock.js --no-hmr --global Unlock",
        "build": "npm run clean; parcel build src/unlock.js --experimental-scope-hoisting --global Unlock",
        "echo": "npm run echo:stop; npm run echo:start",
        "echo:start": "node test/echo.js &",
        "echo:stop": "pkill -f 'node test/echo'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GuyShane/UnlockClient.git"
    },
    "author": "Shane Brass",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GuyShane/UnlockClient/issues"
    },
    "homepage": "https://github.com/GuyShane/UnlockClient#readme",
    "browserslist": [
        "Chrome 78"
    ],
    "devDependencies": {
        "eslint": "^6.7.1",
        "mocha": "^6.2.2",
        "mocha-headless-chrome": "^2.0.3",
        "parcel-bundler": "^1.12.4",
        "sass": "^1.23.7",
        "ws": "^7.2.0"
    },
    "dependencies": {}
}
