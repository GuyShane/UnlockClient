{
    "name": "unlock-client",
    "version": "1.3.3",
    "description": "A frontend javascript library for authenticating with Unlock",
    "keywords": [
        "authentication",
        "frontend",
        "unlock",
        "websocket"
    ],
    "files": [
        "src"
    ],
    "main": "src/unlock.js",
    "scripts": {
        "test": "npm run test:lib; npm run test:min",
        "test:lib": "./node_modules/mocha-headless-chrome/bin/start -f ./test/index.html",
        "test:min": "./node_modules/mocha-headless-chrome/bin/start -f ./test/min.html",
        "gulp": "gulp",
        "echo": "npm run echo:stop; npm run echo:start",
        "echo:start": "node test/echo.js &",
        "echo:stop": "pkill -f 'node test/echo'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GuyShane/UnlockClient.git"
    },
    "author": "Shane Brass",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GuyShane/UnlockClient/issues"
    },
    "homepage": "https://github.com/GuyShane/UnlockClient#readme",
    "devDependencies": {
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.0",
        "gulp-eslint": "^6.0.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.2",
        "gulp-uglify": "^3.0.2",
        "mocha": "^6.2.0",
        "mocha-headless-chrome": "^2.0.3",
        "pump": "^3.0.0",
        "ws": "^7.1.2"
    }
}
